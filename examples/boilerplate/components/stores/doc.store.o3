export component <DocStore
  private[string]:page=loading...>
  <proto type="store" namespace="pages">
    case 'action:getPage':
      try {
        this.page = `/${ctx.route}.html`;
      } catch (error) {
        this.page = `${error.message}`;
        return '';
      }
      break;
  </proto>
</DocStore>

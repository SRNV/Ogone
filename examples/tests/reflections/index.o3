<proto>
  def:
    x: 1
    y: 0
    position:
      name: Test
      origin: 1
      test: []
  default:
    setInterval(() => {
      this.x++;
      this.y--;
      let i = Math.random();
      i > 0.5 ? this.position.test.push(0) : this.position.test.pop();;
    }, 500);
    break;
</proto>
<template>
  <p>${position.name} ${position.origin}</p>
  <p --for="position.test as (item)">
    ${item}
  </p>
</template>
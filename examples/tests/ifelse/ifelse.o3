import component Test from '@/examples/tests/click.o3';

<template>
  <p>${this.count}</p>
  <div --for={t of [1,2,4]} --if={this.count < 3 && t !== 1}>div 1 ${t}</div>
  <Test
    --else-if={this.count >= 3}
    --for={t of [1, 3]}>3eme doit remplacer la Test 1</Test>
</template>

<proto type="app">
  def:
    count: 5
    art:
      - 1
      - 2
      - -1
  default:
    setInterval(() => {
        this.count++;
        if (this.count> 6) {
          this.count = 0;
        }
    }, 500);
</proto>
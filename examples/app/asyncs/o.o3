<proto type="async">
  declare:
    public data: any = 'World';
  default:
    console.warn(this.data);
</proto>
<template>
  <!-- nodetypes 1, 3, 8 supported -->
  Hello <span>${ data }</span>
</template>
<style>
  // scoped by default
  // add 'global' attribute to make it global
  span {
    color: purple;
  }
</style>
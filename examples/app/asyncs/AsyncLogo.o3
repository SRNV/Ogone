
/**
 *  @name AsyncLogo
 *  @description
 *    this component will render after the image is loaded
 *  @example
      Async.resolve(undefined)
 */
<template>
  <div class="container">
    <img --await class="img" src="/ogone.svg"/>
  </div>
</template>
<proto type="async">
  default:
    Async.resolve();
    break;
</proto>
<style>
  @const rule = div {
    width: 60px;
    height: auto;
  }
  .img, .container {
    ...rule
  }
</style>

use @/newapp/tests/tests.o3 as 'test-el';
<p>${count}</p>
<div --if="count < 3">div 1</div>
<test-el --for="art as (r)" --else-if="count + r === 5">2eme doit remplacer la div 1</test-el>
<div --else>3eme doit remplacer la div 1</div>
<proto>
  def:
    count: 0
    art:
      - 1
      - 2
      - -1
  default:
    setInterval(() => {
        this.count++;
        if (this.count> 6) {
            this.count = 0;
            console.warn('refresh')
        }
    }, 500);
</proto>